# Edit this file and then "source" it for Open Horizon

# Target hardware arch for Open Horizon (get this from `hzn architecture`)
export ARCH=amd64

# Credentials for your cluster node (you can make up almost anything here)
export HZN_EXCHANGE_NODE_AUTH="node:token"

# Credentials (get these from your Open Horizon system administrator)
export HZN_ORG_ID=telus
export HZN_EXCHANGE_USER_AUTH="iamapikey:"
export HZN_EXCHANGE_URL=
export HZN_FSS_CSSURL=

# A name and version for the Open Horizon service
export SERVICE_NAME=
export SERVICE_VERSION=1.0.0
